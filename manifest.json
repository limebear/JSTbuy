{
  "name": "JSTbuy - Hide upbit sell button",
  "description": "Chrome extension : Hide the sell tab in Upbit",
  "version": "0.1",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.upbit.com/exchange*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "JSTbuy - Hide upbit sell button",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}